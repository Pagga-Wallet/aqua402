        "rfq.CreateRFQRequest": {
            "type": "object",
            "properties": {
                "amount": {
                    "type": "string"
                },
                "borrower_address": {
                    "type": "string"
                },
                "collateral_type": {
                    "type": "integer"
                },
                "duration": {
                    "type": "integer"
                },
                "flow_description": {
                    "type": "string"
                }
            }
        },
        "rfq.QuoteRequest": {
